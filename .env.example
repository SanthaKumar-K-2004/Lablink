# Supabase Configuration
SUPABASE_PROJECT_REF=<your-project-ref>
SUPABASE_URL=https://<your-project-ref>.supabase.co
SUPABASE_ANON_KEY=<your-anon-key>
SUPABASE_SERVICE_ROLE_KEY=<your-service-role-key>
SUPABASE_JWT_SECRET=<your-jwt-secret>
SUPABASE_DB_PASSWORD=<your-db-password>
SUPABASE_STUDIO_URL=https://supabase.com/dashboard/project/<your-project-ref>
SUPABASE_FUNCTIONS_URL=https://<your-project-ref>.supabase.co/functions/v1
SUPABASE_REALTIME_URL=wss://<your-project-ref>.supabase.co/realtime/v1

# LabLink Application Configuration
LABLINK_QR_JWT_SECRET=<secret-for-tamper-proof-qr-code-hashing>
LABLINK_SMTP_API_KEY=<sendgrid-api-key-or-smtp-token>
LABLINK_SENDGRID_API_KEY=<sendgrid-api-key>
LABLINK_NOTIFICATION_EMAIL_FROM=notifications@lablink.app
LABLINK_NOTIFICATION_EMAIL_NAME=LabLink Alerts
LABLINK_TWILIO_ACCOUNT_SID=<twilio-account-sid>
LABLINK_TWILIO_AUTH_TOKEN=<twilio-auth-token>
LABLINK_TWILIO_FROM_NUMBER=+15551234567
LABLINK_FCM_SERVER_KEY=<firebase-cloud-messaging-server-key>
LABLINK_DEFAULT_NOTIFICATION_CHANNELS=in_app,email
LABLINK_FUNCTION_TIMEOUT_MS=30000
APP_BASE_URL=http://localhost:8080
APP_PROD_URL=https://lablink.app
FLUTTER_WEB_ALLOWED_ORIGINS=http://localhost:8080,https://lablink.app

# Optional: S3/Storage Adapter (if not using Supabase Storage)
# S3_HOST=<your-s3-bucket-url>
